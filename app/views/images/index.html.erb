
<%= form_with scope: :image, url: images_path, local: true do |form| %>
    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
    </p>

    <p>
      <%= form.label :link %><br>
      <%= form.text_area :link %>
    </p>

    <p>
      <%= form.submit %>
    </p>
<% end %>
